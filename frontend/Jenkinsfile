pipeline {
  agent {
    docker { image 'node:18.17.1' }
  }
  stages {
    stage('Requirements') {
      steps {
        dir('frontend') {
          sh "sudo chown -R 113:121 "/.npm""
          sh 'npm install'
        }
      }
    }
      stage('Build') {
        steps {
          dir('frontend') {
            sh 'npm run build'
          }
        }
      }
  }
}
